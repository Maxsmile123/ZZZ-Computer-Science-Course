#include <stdbool.h>
#include <stdio.h>

bool check_digits(int32_t number) {
    int32_t digit = number % 10;
    number /= 10;
    while (number > 0) {
        int32_t next_digit = number % 10;
        if (digit == next_digit) {
            return false;
        }
        digit = next_digit;
        number /= 10;
    }
    return true;
}

int Task() {
    int32_t number = 646564;
    if (check_digits(number)) {
        printf("YES");
    } else {
        printf("NO");
    }
    return 0;
}
